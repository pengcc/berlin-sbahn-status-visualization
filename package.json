{
  "name": "s-bahn-status",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,html,css,scss,md}\"",
    "prettier:staged": "prettier --write $(git diff --name-only --cached --diff-filter=ACM)",
    "check": "prettier --check \"src/**/*.{js,jsx,html,css,scss,md}\" --log-level=debug",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --check --ignore-path .prettierignore"
    ],
    "*.{css,scss,json,md,html}": [
      "prettier --check --ignore-path .prettierignore"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "recharts": "^2.7.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.0.0",
    "sass": "^1.64.1",
    "vite": "^4.4.5"
  },
  "volta": {
    "node": "22.18.0"
  }
}
